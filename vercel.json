{
  "version": 2,
  "builds": [
    {
      "src": "server-ranziro.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "server-ranziro.js",
      "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store"
            }
          ]
        }
      ]
    }
  ]
}
