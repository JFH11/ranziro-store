{
    "version": 2,
    "builds": [
      { "src": "server-ranziro.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server-ranziro.js" }
    ],
    "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store",
          "cleanUrls": true
        }
    ]  
  }